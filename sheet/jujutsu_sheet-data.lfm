		</layout>
		<dataLink fields="{'outros_vontade', 'maestria_vontade', 'especializacao_vontade', 'sheet.valor_nivel', 'maestria_nivel', 'base_carisma'}">
			<event name="onPersistedChange"><![CDATA[

				local valoroutros = tonumber(sheet.outros_vontade) or 0
				local valornivel = tonumber(sheet.valor_nivel) or 0
				local valormaestria = tonumber(sheet.maestria_nivel) or 0 
				local valormodificador = tonumber(sheet.modificadorBase_carisma) or 0

				if sheet.especializacao_vontade == true then
					sheet.total_vontade = math.floor(valornivel/2) + math.floor((valormaestria+valormodificador)*1.5) + valoroutros;
				elseif sheet.maestria_vontade == true then
					sheet.total_vontade = math.floor(valornivel/2) + valormaestria + valormodificador + valoroutros;
				else
					sheet.total_vontade = math.floor(valornivel/2) + valormodificador + valoroutros;
				end

			]]></event>
		</dataLink>

