	<!-- MODIFICADORES DE VALORES BASE -->

	<layout left="481.86078" top="426.90628" height="60.47377" width="90.469223">
    	<label align="client" horzTextAlign="center" field="modificadorBase_carisma" fontSize="45" fontStyle="bold" fontColor="white" fontFamily="Chiller"/>
	</layout>
	<dataLink field="base_carisma">
		<event name="onChange"><![CDATA[
            local valorcarisma = tonumber(sheet.base_carisma)
			local modcarisma = math.floor(((valorcarisma)-10) / 2);
            if valorcarisma >= 10 then
                sheet.modificadorBase_carisma = "+".. modcarisma
            else
                sheet.modificadorBase_carisma = modcarisma
            end;
        ]]></event>
	</dataLink>

