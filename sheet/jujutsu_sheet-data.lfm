	<layout left="502.08533" top="433.09241" height="50.030956" width="50.030956">
		<label align="client" horzTextAlign="center" field="modificadorBase_carisma" />
	</layout>
	<dataLink field="base_carisma">
		<event name="onChange"><![CDATA[
			local valorcarisma = tonumber(sheet.base_carisma)
			if valorcarisma >= 10 then
				sheet.modificadorBase_carisma = math.floor(((valorcarisma)-10) / 2);
			else
				sheet.modificadorBase_carisma = math.ceil(((valorcarisma) - 10) / 2);
			end;
        ]]></event>
	</dataLink>

